<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.collections.FXCollections?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<AnchorPane id="AnchorPane" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
            minWidth="-Infinity" prefHeight="350.0" prefWidth="300.0" styleClass="background"
            stylesheets="@../../../view/fxml/background.css" xmlns="http://javafx.com/javafx/8"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="app.board.controller.EditSprintController">
    <children>
        <GridPane prefHeight="192.0" prefWidth="260.0" AnchorPane.leftAnchor="20.0"
                  AnchorPane.rightAnchor="20.0" AnchorPane.topAnchor="20.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="129.0" minWidth="10.0"
                                   prefWidth="92.0"/>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="168.0" minWidth="10.0"
                                   prefWidth="168.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <Label text="Name" textFill="#3f7cff" GridPane.columnIndex="0"
                   GridPane.rowIndex="1">
                <font>
                    <Font name="Ubuntu" size="15.0" fx:id="x1"/>
                </font>
            </Label>
            <TextField fx:id="nameField" opacity="0.8" prefHeight="25.0" prefWidth="130.0"
                       GridPane.columnIndex="1" GridPane.rowIndex="1"/>
            <Label font="$x1" text="Project" textFill="#3f7cff" GridPane.columnIndex="0"
                   GridPane.rowIndex="2"/>
            <ComboBox id="project" fx:id="projectSelector" opacity="0.8" prefHeight="25.0"
                      prefWidth="168.0" GridPane.columnIndex="1" GridPane.rowIndex="2">
                <items>
                    <FXCollections fx:factory="observableArrayList">
                        <String fx:value="Item 1"/>
                        <String fx:value="Item 2"/>
                        <String fx:value="Item 3"/>
                    </FXCollections>
                </items>
            </ComboBox>
            <Label text="Edit" textFill="#3f7cff" GridPane.columnIndex="0"
                   GridPane.rowIndex="0">
                <font>
                    <Font name="Ubuntu Bold" size="15.0" fx:id="x2"/>
                </font>
            </Label>
            <ComboBox id="sprind" fx:id="sprintSelector" onAction="#select" opacity="0.8"
                      prefHeight="25.0" prefWidth="168.0" GridPane.columnIndex="1"
                      GridPane.rowIndex="0">
                <items>
                    <FXCollections fx:factory="observableArrayList">
                        <String fx:value="Item 1"/>
                        <String fx:value="Item 2"/>
                        <String fx:value="Item 3"/>
                    </FXCollections>
                </items>
            </ComboBox>
            <Label font="$x1" text="Start date" textFill="#3f7cff" GridPane.rowIndex="3"/>
            <DatePicker fx:id="startPicker" opacity="0.8" GridPane.columnIndex="1"
                        GridPane.rowIndex="3"/>
            <Label font="$x1" text="Goal" textFill="#3f7cff" GridPane.rowIndex="5"/>
            <Label font="$x1" text="End date" textFill="#3f7cff" GridPane.rowIndex="4"/>
            <DatePicker fx:id="endDatePicker" opacity="0.8" GridPane.columnIndex="1"
                        GridPane.rowIndex="4"/>
        </GridPane>
        <Button font="$x2" layoutY="300.0" mnemonicParsing="false" onAction="#handleSubmit"
                opacity="0.8" text="Submit" AnchorPane.leftAnchor="65.0"/>
        <TextArea fx:id="goalArea" layoutX="21.0" layoutY="208.0" opacity="0.8" prefHeight="85.0"
                  prefWidth="259.0" wrapText="true" AnchorPane.leftAnchor="21.0"
                  AnchorPane.rightAnchor="20.0"/>
        <BorderPane prefHeight="25.0" prefWidth="300.0" AnchorPane.bottomAnchor="0.0"
                    AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
            <center>
                <Label fx:id="errorLabel" font="$x1" text="" textFill="RED"/>
            </center>
        </BorderPane>
        <Button contentDisplay="CENTER" font="$x2" layoutY="300.0" mnemonicParsing="false"
                onAction="#handleDelete" opacity="0.8" text="Delete" AnchorPane.rightAnchor="65.0"/>
    </children>
</AnchorPane>
